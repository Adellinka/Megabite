    <input\r\n                type=\"text\"\r\n                id=\"name\"\r\n                placeholder=\"Enter name\"\r\n                required\r\n                onChange={(e) => setName(e.target.value)}\r\n              />\r\n              <label htmlFor=\"email\">Email</label>\r\n              <input\r\n                type=\"text\"\r\n                id=\"email\"\r\n                placeholder=\"Enter email\"\r\n                required\r\n                onChange={(e) => setEmail(e.target.value)}\r\n              />\r\n              <label htmlFor=\"password\">Password</label>\r\n              <input\r\n                type=\"password\"\r\n                id=\"password\"\r\n                placeholder=\"Enter password\"\r\n                required\r\n                onChange={(e) => setPassword(e.target.value)}\r\n              />\r\n              <label htmlFor=\"confirmPassword\">Password</label>\r\n              <input\r\n                type=\"password\"\r\n                id=\"confirmPassword\"\r\n                placeholder=\"Enter confirm password\"\r\n                required\r\n                onChange={(e) => setConfirmPassword(e.target.value)}\r\n              />\r\n            </div>\r\n            <div className=\"button-wrapper\">\r\n              <button className=\"button\" type=\"submit\">\r\n                <FontAwesomeIcon icon={faArrowRight} size=\"2x\" />\r\n              </button>\r\n            </div>\r\n          </form>\r\n        </div>\r\n      </section>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default ModalRegister;\r\n","D:\\Adel\\Adeel\\School\\VOŠ\\Absolventka\\Megabite\\frontend\\src\\components\\MessageBox.js",[],"D:\\Adel\\Adeel\\School\\VOŠ\\Absolventka\\Megabite\\frontend\\src\\components\\LoadingBox.js",[],"D:\\Adel\\Adeel\\School\\VOŠ\\Absolventka\\Megabite\\frontend\\src\\actions\\userActions.js",[],"D:\\Adel\\Adeel\\School\\VOŠ\\Absolventka\\Megabite\\frontend\\src\\constants\\userConstants.js",[],"D:\\Adel\\Adeel\\